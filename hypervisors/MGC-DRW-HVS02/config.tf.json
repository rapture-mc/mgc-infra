{
  "module": {
    "filebrowser-server": {
      "autostart": true,
      "bridge": "br0",
      "dhcp": true,
      "memory": "4096",
      "source": "git::https://github.com/rapture-mc/terraform-libvirt-module.git?ref=40acff807a0ffb1c0da741774c37ebeda90730b7",
      "system_volume": 300,
      "vcpu": 2,
      "vm_count": 1,
      "vm_hostname_prefix": "MGC-DRW-FBR"
    },
    "semaphore-server": {
      "autostart": true,
      "bridge": "br0",
      "dhcp": true,
      "memory": "4096",
      "source": "git::https://github.com/rapture-mc/terraform-libvirt-module.git?ref=40acff807a0ffb1c0da741774c37ebeda90730b7",
      "system_volume": 300,
      "vcpu": 2,
      "vm_count": 1,
      "vm_hostname_prefix": "MGC-DRW-SEM"
    },
    "windows-server": {
      "autostart": true,
      "bridge": "br0",
      "dhcp": true,
      "memory": "4096",
      "os_img_url": "/var/lib/libvirt/images/win22.qcow2",
      "source": "git::https://github.com/rapture-mc/terraform-libvirt-module.git?ref=40acff807a0ffb1c0da741774c37ebeda90730b7",
      "system_volume": 300,
      "uefi_enabled": false,
      "vcpu": 2,
      "vm_count": 1,
      "vm_hostname_prefix": "MGC-DRW-DMC"
    }
  },
  "provider": {
    "libvirt": {
      "uri": "qemu+ssh://controller@192.168.1.16/system"
    }
  },
  "terraform": {
    "required_providers": {
      "libvirt": {
        "source": "dmacvicar/libvirt"
      }
    }
  }
}
